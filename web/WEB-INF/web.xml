<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Parse</servlet-name>
        <servlet-class>sjtu.edu.ParseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Parse</servlet-name>
        <url-pattern>/parse</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>uploadParse</servlet-name>
        <servlet-class>sjtu.edu.UploadParseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadParse</servlet-name>
        <url-pattern>/uploadParse</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>bufferParse</servlet-name>
    <servlet-class>sjtu.edu.BufferParseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bufferParse</servlet-name>
        <url-pattern>/bufferParse</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>